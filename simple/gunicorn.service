[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/aws-codedeploy
Environment="PATH=/home/ubuntu/aws-codedeploy/accure/bin"
ExecStart=/home/ubuntu/aws-codedeploy/acccure/bin/gunicorn --workers 3 --bind unix:myproject.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target